<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="tutor_dashboard">
  <grid id="mainPanel" binding="contentPane" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="15" left="15" bottom="15" right="15"/>
    <constraints>
      <xy x="20" y="20" width="950" height="750"/>
    </constraints>
    <properties>
      <background color="-1"/>
    </properties>
    <border type="none"/>
    <children>
      <!-- Enhanced Header Panel -->
      <grid id="headerPanel" layout-manager="GridLayoutManager" row-count="1" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="10" left="10" bottom="10" right="10"/>
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="-1" height="60"/>
          </grid>
        </constraints>
        <properties>
          <background color="-16777216"/>
        </properties>
        <border type="line" title="">
          <color color="-16777216"/>
        </border>
        <children>
          <hspacer id="leftSpacer">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
          </hspacer>
          <component id="welcomeLabel" class="javax.swing.JLabel" binding="welcomeLabel">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="18" style="1"/>
              <foreground color="-1"/>
              <horizontalAlignment value="0"/>
              <text value="Welcome, Tutor"/>
            </properties>
          </component>
          <component id="logoutButton" class="javax.swing.JButton" binding="logoutButton">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false">
                <preferred-size width="100" height="35"/>
              </grid>
            </constraints>
            <properties>
              <background color="-65536"/>
              <font size="12" style="1"/>
              <foreground color="-16777216"/>
              <text value="Logout"/>
            </properties>
          </component>
        </children>
      </grid>
      <!-- Enhanced Tabbed Pane -->
      <tabbedpane id="tabbedPane" binding="tabbedPane">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="200" height="200"/>
          </grid>
        </constraints>
        <properties>
          <font size="14"/>
        </properties>
        <border type="none"/>
        <children>
          <!-- Profile Tab -->
          <grid id="profilePanel" binding="profilePanel" layout-manager="GridLayoutManager" row-count="7" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="👤 Profile"/>
            </constraints>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="profileHeaderLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="16" style="1"/>
                  <foreground color="-16777216"/>
                  <text value="👤 My Profile Information"/>
                </properties>
              </component>
              <component id="nameLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <text value="👤 Name:"/>
                </properties>
              </component>
              <component id="nameField" class="javax.swing.JTextField" binding="nameField">
                <constraints>
                  <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <editable value="false"/>
                  <font size="12"/>
                </properties>
              </component>
              <component id="emailLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <text value="📧 Email:"/>
                </properties>
              </component>
              <component id="emailField" class="javax.swing.JTextField" binding="emailField">
                <constraints>
                  <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="12"/>
                </properties>
              </component>
              <component id="contactLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <text value="📞 Contact:"/>
                </properties>
              </component>
              <component id="contactField" class="javax.swing.JTextField" binding="contactField">
                <constraints>
                  <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="12"/>
                </properties>
              </component>
              <component id="subjectsLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <text value="📚 Subjects:"/>
                </properties>
              </component>
              <scrollpane id="subjectsScrollPane">
                <constraints>
                  <grid row="4" column="1" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="-1" height="100"/>
                  </grid>
                </constraints>
                <properties/>
                <border type="line">
                  <color color="-4473925"/>
                </border>
                <children>
                  <component id="subjectsList" class="javax.swing.JList" binding="subjectsList">
                    <constraints/>
                    <properties>
                      <font size="12"/>
                    </properties>
                  </component>
                </children>
              </scrollpane>
              <component id="levelsLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="5" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <text value="🎓 Levels:"/>
                </properties>
              </component>
              <scrollpane id="levelsScrollPane">
                <constraints>
                  <grid row="5" column="1" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="-1" height="100"/>
                  </grid>
                </constraints>
                <properties/>
                <border type="line">
                  <color color="-4473925"/>
                </border>
                <children>
                  <component id="levelsList" class="javax.swing.JList" binding="levelsList">
                    <constraints/>
                    <properties>
                      <font size="12"/>
                    </properties>
                  </component>
                </children>
              </scrollpane>
              <grid id="profileButtonPanel" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="20" left="0" bottom="0" right="0"/>
                <constraints>
                  <grid row="6" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <hspacer id="profileSpacer">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                  </hspacer>
                  <component id="updateProfileButton" class="javax.swing.JButton" binding="updateProfileButton">
                    <constraints>
                      <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                        <preferred-size width="150" height="35"/>
                      </grid>
                    </constraints>
                    <properties>
                      <background color="-16751002"/>
                      <font size="12" style="1"/>
                      <foreground color="-16777216"/>
                      <text value="💾 Update Profile"/>
                    </properties>
                  </component>
                </children>
              </grid>
            </children>
          </grid>
          <!-- Classes Tab -->
          <grid id="classesPanel" binding="classesPanel" layout-manager="GridLayoutManager" row-count="3" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="📚 Classes"/>
            </constraints>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="classesHeaderLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="16" style="1"/>
                  <foreground color="-16777216"/>
                  <text value="📚 My Classes"/>
                </properties>
              </component>
              <scrollpane id="classesScrollPane">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="-1" height="300"/>
                  </grid>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="classesTable" class="javax.swing.JTable" binding="classesTable">
                    <constraints/>
                    <properties>
                      <font size="12"/>
                      <gridColor color="-4473925"/>
                      <rowHeight value="25"/>
                      <selectionBackground color="-16751002"/>
                    </properties>
                  </component>
                </children>
              </scrollpane>
              <grid id="classButtonsPanel" layout-manager="GridLayoutManager" row-count="1" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="10" left="0" bottom="0" right="0"/>
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <background color="-1"/>
                </properties>
                <border type="none"/>
                <children>
                  <component id="addClassButton" class="javax.swing.JButton" binding="addClassButton">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                        <preferred-size width="150" height="35"/>
                      </grid>
                    </constraints>
                    <properties>
                      <background color="-16751002"/>
                      <font size="12" style="1"/>
                      <foreground color="-16777216"/>
                      <text value="➕ Add Class"/>
                    </properties>
                  </component>
                  <component id="updateClassButton" class="javax.swing.JButton" binding="updateClassButton">
                    <constraints>
                      <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                        <preferred-size width="150" height="35"/>
                      </grid>
                    </constraints>
                    <properties>
                      <background color="-39424"/>
                      <font size="12" style="1"/>
                      <foreground color="-16777216"/>
                      <text value="✏️ Update Class"/>
                    </properties>
                  </component>
                  <component id="deleteClassButton" class="javax.swing.JButton" binding="deleteClassButton">
                    <constraints>
                      <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                        <preferred-size width="150" height="35"/>
                      </grid>
                    </constraints>
                    <properties>
                      <background color="-65536"/>
                      <font size="12" style="1"/>
                      <foreground color="-16777216"/>
                      <text value="🗑️ Delete Class"/>
                    </properties>
                  </component>
                </children>
              </grid>
            </children>
          </grid>
          <!-- Students Tab -->
          <grid id="studentsPanel" binding="studentsPanel" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="👥 Students"/>
            </constraints>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="studentsHeaderLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="16" style="1"/>
                  <foreground color="-16777216"/>
                  <text value="👥 My Students"/>
                </properties>
              </component>
              <scrollpane id="studentsScrollPane">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="-1" height="400"/>
                  </grid>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="studentsTable" class="javax.swing.JTable" binding="studentsTable">
                    <constraints/>
                    <properties>
                      <font size="12"/>
                      <gridColor color="-4473925"/>
                      <rowHeight value="25"/>
                      <selectionBackground color="-16751002"/>
                    </properties>
                  </component>
                </children>
              </scrollpane>
            </children>
          </grid>
        </children>
      </tabbedpane>
    </children>
  </grid>
</form>
