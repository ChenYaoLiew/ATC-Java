<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="student_dashboard">
  <grid id="mainPanel" binding="mainPanel" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="15" left="15" bottom="15" right="15"/>
    <constraints>
      <xy x="20" y="20" width="950" height="750"/>
    </constraints>
    <properties>
      <background color="-1"/>
    </properties>
    <border type="none"/>
    <children>
      <!-- Enhanced Header Panel -->
      <grid id="headerPanel" layout-manager="GridLayoutManager" row-count="1" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="10" left="10" bottom="10" right="10"/>
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="-1" height="60"/>
          </grid>
        </constraints>
        <properties>
          <background color="-16777216"/>
        </properties>
        <border type="line" title="">
          <color color="-16777216"/>
        </border>
        <children>
          <hspacer id="leftSpacer">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
          </hspacer>
          <component id="welcomeLabel" class="javax.swing.JLabel" binding="welcomeLabel">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="18" style="1"/>
              <foreground color="-1"/>
              <horizontalAlignment value="0"/>
              <text value="Welcome, Student"/>
            </properties>
          </component>
          <component id="logoutButton" class="javax.swing.JButton" binding="logoutButton">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false">
                <preferred-size width="100" height="35"/>
              </grid>
            </constraints>
            <properties>
              <background color="-65536"/>
              <font size="12" style="1"/>
              <foreground color="-16777216"/>
              <text value="Logout"/>
            </properties>
          </component>
        </children>
      </grid>
      <!-- Enhanced Tabbed Pane -->
      <tabbedpane id="tabbedPane" binding="tabbedPane">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="200" height="200"/>
          </grid>
        </constraints>
        <properties>
          <font size="14"/>
        </properties>
        <border type="none"/>
        <children>
          <!-- Enhanced Schedule Tab -->
          <grid id="scheduleTab" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="📅 My Schedule"/>
            </constraints>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="scheduleHeaderLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="16" style="1"/>
                  <foreground color="-16777216"/>
                  <text value="📚 Weekly Class Schedule"/>
                </properties>
              </component>
              <scrollpane id="scheduleScrollPane">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="-1" height="400"/>
                  </grid>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="scheduleTable" class="javax.swing.JTable" binding="scheduleTable">
                    <constraints/>
                    <properties>
                      <font size="12"/>
                      <gridColor color="-12566464"/>
                      <rowHeight value="25"/>
                      <selectionBackground color="-16751002"/>
                    </properties>
                  </component>
                </children>
              </scrollpane>
            </children>
          </grid>
          <!-- Enhanced Subject Request Tab -->
          <grid id="subjectRequestTab" layout-manager="GridLayoutManager" row-count="5" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="📝 Subject Requests"/>
            </constraints>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="requestHeaderLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="16" style="1"/>
                  <foreground color="-16777216"/>
                  <text value="✏️ Submit New Subject Change Request"/>
                </properties>
              </component>
              <component id="requestLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="12"/>
                  <text value="Describe your subject change request (e.g., add/drop subjects, change class time):"/>
                </properties>
              </component>
              <scrollpane id="requestScrollPane">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="-1" height="100"/>
                  </grid>
                </constraints>
                <properties/>
                <border type="line"/>
                <children>
                  <component id="requestTextArea" class="javax.swing.JTextArea" binding="requestTextArea">
                    <constraints/>
                    <properties>
                      <font size="12"/>
                      <lineWrap value="true"/>
                      <rows value="4"/>
                      <wrapStyleWord value="true"/>
                    </properties>
                  </component>
                </children>
              </scrollpane>
              <grid id="buttonPanel" layout-manager="GridLayoutManager" row-count="1" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="10" left="0" bottom="10" right="0"/>
                <constraints>
                  <grid row="3" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                                     <component id="sendRequestButton" class="javax.swing.JButton" binding="sendRequestButton">
                     <constraints>
                       <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                         <preferred-size width="150" height="35"/>
                       </grid>
                     </constraints>
                     <properties>
                       <background color="-16751002"/>
                       <font size="12" style="1"/>
                       <foreground color="-16777216"/>
                       <text value="📤 Send Request"/>
                     </properties>
                   </component>
                  <hspacer id="buttonSpacer">
                    <constraints>
                      <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                  </hspacer>
                                     <component id="deleteRequestButton" class="javax.swing.JButton" binding="deleteRequestButton">
                     <constraints>
                       <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                         <preferred-size width="180" height="35"/>
                       </grid>
                     </constraints>
                     <properties>
                       <background color="-39424"/>
                       <font size="12" style="1"/>
                       <foreground color="-16777216"/>
                       <text value="🗑️ Delete Pending"/>
                     </properties>
                   </component>
                </children>
              </grid>
              <grid id="requestTablePanel" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="10" left="0" bottom="0" right="0"/>
                <constraints>
                  <grid row="4" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="requestTableLabel" class="javax.swing.JLabel">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                    </constraints>
                    <properties>
                      <font size="14" style="1"/>
                      <text value="📋 My Request History"/>
                    </properties>
                  </component>
                  <scrollpane id="requestTableScrollPane">
                    <constraints>
                      <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                        <preferred-size width="-1" height="200"/>
                      </grid>
                    </constraints>
                    <properties/>
                    <border type="none"/>
                    <children>
                      <component id="requestTable" class="javax.swing.JTable" binding="requestTable">
                        <constraints/>
                        <properties>
                          <font size="11"/>
                          <gridColor color="-12566464"/>
                          <rowHeight value="25"/>
                          <selectionBackground color="-16751002"/>
                        </properties>
                      </component>
                    </children>
                  </scrollpane>
                </children>
              </grid>
            </children>
          </grid>
          <!-- Enhanced Payments Tab -->
          <grid id="paymentsTab" layout-manager="GridLayoutManager" row-count="3" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="💰 Payments"/>
            </constraints>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="paymentHeaderLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="16" style="1"/>
                  <foreground color="-16777216"/>
                  <text value="💳 Payment Status &amp; History"/>
                </properties>
              </component>
              <component id="paymentStatusLabel" class="javax.swing.JLabel" binding="paymentStatusLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <foreground color="-39424"/>
                  <text value="💰 Payment Status and Balance"/>
                </properties>
              </component>
              <scrollpane id="paymentScrollPane">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="7" hsize-policy="7" anchor="0" fill="3" indent="0" use-parent-layout="false">
                    <preferred-size width="-1" height="400"/>
                  </grid>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="paymentTable" class="javax.swing.JTable" binding="paymentTable">
                    <constraints/>
                    <properties>
                      <font size="12"/>
                      <gridColor color="-12566464"/>
                      <rowHeight value="25"/>
                      <selectionBackground color="-16751002"/>
                    </properties>
                  </component>
                </children>
              </scrollpane>
            </children>
          </grid>
          <!-- Enhanced Profile Tab -->
          <grid id="profileTab" layout-manager="GridLayoutManager" row-count="4" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="15" left="15" bottom="15" right="15"/>
            <constraints>
              <tabbedpane title="👤 Profile"/>
            </constraints>
            <properties>
              <background color="-1"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="profileHeaderLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="16" style="1"/>
                  <foreground color="-16777216"/>
                  <text value="👤 My Profile Information"/>
                </properties>
              </component>
              <component id="nameLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false">
                    <preferred-size width="100" height="-1"/>
                  </grid>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <text value="👤 Name:"/>
                </properties>
              </component>
              <component id="profileNameField" class="javax.swing.JTextField" binding="profileNameField">
                <constraints>
                  <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="300" height="30"/>
                  </grid>
                </constraints>
                <properties>
                  <font size="12"/>
                </properties>
              </component>
              <component id="contactLabel" class="javax.swing.JLabel">
                <constraints>
                  <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false">
                    <preferred-size width="100" height="-1"/>
                  </grid>
                </constraints>
                <properties>
                  <font size="14" style="1"/>
                  <text value="📞 Contact:"/>
                </properties>
              </component>
              <component id="profileContactField" class="javax.swing.JTextField" binding="profileContactField">
                <constraints>
                  <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                    <preferred-size width="300" height="30"/>
                  </grid>
                </constraints>
                <properties>
                  <font size="12"/>
                </properties>
              </component>
              <grid id="profileButtonPanel" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                <margin top="20" left="0" bottom="0" right="0"/>
                <constraints>
                  <grid row="3" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties/>
                <border type="none"/>
                <children>
                  <hspacer id="profileSpacer">
                    <constraints>
                      <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
                    </constraints>
                  </hspacer>
                                     <component id="updateProfileButton" class="javax.swing.JButton" binding="updateProfileButton">
                     <constraints>
                       <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                         <preferred-size width="150" height="35"/>
                       </grid>
                     </constraints>
                     <properties>
                       <background color="-16751002"/>
                       <font size="12" style="1"/>
                       <foreground color="-16777216"/>
                       <text value="💾 Update Profile"/>
                     </properties>
                   </component>
                </children>
              </grid>
            </children>
          </grid>
        </children>
      </tabbedpane>
    </children>
  </grid>
</form>

